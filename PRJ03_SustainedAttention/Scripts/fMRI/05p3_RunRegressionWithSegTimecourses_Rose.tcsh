set output_dir = ${PRJDIR}/Results/${subj}/${outFolder}

# enter the results directory (can begin processing data)
cd $output_dir

# detect number of runs by counting pb05 datasets
set nRuns = `ls pb05.$subj.r*.meica.nii | wc -l`
echo "Found ${nRuns} runs for subject ${subj}."
		
        
        
# create bandpass regressors (instead of using 3dBandpass, say)
# (make separate regressors per run, with all in one file)
# set list of runs
set runs = (`count -digits 2 1 ${nRuns}`)
# make note of repetitions (TRs) per run
# set tr_counts = ( 237 237 237 237 ) 
set nTRs = `3dinfo -nv pb00.$subj.r${runs[1]}_e1.tcat`
set tr_counts = (`yes $nTRs | head -n $nRuns`)
foreach index ( `count -digits 1 1 $#runs` )
    set nt = $tr_counts[$index]
    set run = $runs[$index]
    1dBport -nodata $nt 2 -band 0.01 0.1 -invert -nozero > rm.bpass.1D
    1d_tool.py -infile rm.bpass.1D -pad_into_many_runs $run $#runs            \
               -set_run_lengths $tr_counts -overwrite                                   \
               -write bpass.r$run.1D
end
1dcat bpass.r*1D > bandpass_rall.1D
        
# ================================ regress =================================
# assume 05_ has already been run, so the setup files have already been written.

# compute motion squares (for use in regression)
foreach index ( `count -digits 1 0 5` )    
    1deval -a motion_demean.1D"[$index]" -expr 'a*a' > rm.motion_demean_sq${index}.1D
end
1dcat rm.motion_demean_sq*1D > motion_demean_sqall.1D
1d_tool.py -infile motion_demean_sqall.1D -set_nruns ${nRuns}                                 \
           -demean -overwrite -write motion_demean_sq.1D
# convert motion parameters for per-run regression
1d_tool.py -infile motion_demean_sq.1D -set_nruns ${nRuns} -overwrite                               \
           -split_into_pad_runs mot_demean_sq

# compute motion derivative squares (for use in regression)
foreach index ( `count -digits 1 0 5` )    
    1deval -a motion_deriv.1D"[$index]" -expr 'a*a' > rm.motion_deriv_sq${index}.1D
end
1dcat rm.motion_deriv_sq*1D > motion_deriv_sqall.1D
1d_tool.py -infile motion_deriv_sqall.1D -set_nruns ${nRuns}                                 \
           -demean -overwrite -write motion_deriv_sq.1D
